{
  "name": "@kaleidawave/prism",
  "version": "1.4.2",
  "description": "Isomorphic web app compiler",
  "main": "./lib/node/index.js",
  "scripts": {
    "test": "jest",
    "build": "packemon build --addEngines",
    "build-tsc": "tsc",
    "build-bundle": "node inject-bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaleidawave/prism.git"
  },
  "keywords": [
    "web-components",
    "ssr",
    "template",
    "isomorphic",
    "framework",
    "compiler"
  ],
  "author": "kaleidawave",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaleidawave/prism/issues"
  },
  "files": [
    "out"
  ],
  "homepage": "https://github.com/kaleidawave/prism#readme",
  "// Only used for testing and development:": "",
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/jest-cli": "^24.3.0",
    "@types/node": "^13.13.25",
    "jest": "^25.5.4",
    "jest-cli": "^26.5.3",
    "packemon": "^0.10.1",
    "ts-jest": "^26.4.1",
    "typescript": "^3.9.7"
  },
  "packemon": [
    {
      "inputs": {
        "index": "src/node.ts"
      },
      "platform": "node"
    },
    {
      "inputs": {
        "bin": "src/cli.ts"
      },
      "platform": "node"
    },
    {
      "inputs": {
        "web": "src/web.ts"
      },
      "platform": "browser",
      "support": "current"
    }
  ],
  "jest": {
    "preset": "ts-jest"
  },
  "engines": {
    "node": ">=10.3.0",
    "npm": ">=6.1.0"
  },
  "dependencies": {
    "path-browserify": "^1.0.1",
    "regenerator-runtime": "^0.13.7"
  },
  "bin": {
    "prism": "lib/node/bin.js"
  },
  "directories": {
    "example": "examples",
    "test": "tests"
  }
}
